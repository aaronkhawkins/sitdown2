<%
#locals
status = feed_item.item
%>


<div class="feed_item photo_feed" id="<%= feed_item.dom_id %>">
  <div class="left">
	<img src="/images/blurp_photos.png" class="png icon_small" title="User Photo Icon"  alt="User Photo Icon" /> 
	<%= link_to h(status.profile.f), status.profile %> Statused <%= time_ago_in_words status.created_at %> ago <%= link_to sanitize(textilize(status.description)), profile_status_path(status.profile, status) %>
	</div> 
  	<div class="right">
    	<%= x_feed_link(feed_item) if me %>
  	</div>
	<div class="clear"></div>
</div>